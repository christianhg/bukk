{
  "name": "bukk",
  "description": "Functional programming for Norwegians ⛷️",
  "keywords": [
    "functional-programming",
    "fp",
    "curry",
    "data-last",
    "library"
  ],
  "version": "0.10.5",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/bukk.js",
  "repository": "christianhg/bukk",
  "author": {
    "name": "Christian Grøngaard",
    "email": "christianhg@gmail.com"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "npm run lint & nyc ava && webpack",
    "lint": "standard --verbose | snazzy",
    "test": "npm run lint & nyc ava",
    "watch:test": "npm run lint & ava --watch"
  },
  "ava": {
    "files": "tests/*.js",
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "src/*.js"
    ]
  },
  "babel": {
    "presets": [
      "@ava/stage-4"
    ]
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^1.0.0",
    "ava": "^0.18.1",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.22.0",
    "coveralls": "^2.11.15",
    "nyc": "^10.1.2",
    "snazzy": "^6.0.0",
    "standard": "^8.6.0",
    "webpack": "^2.2.1"
  }
}
